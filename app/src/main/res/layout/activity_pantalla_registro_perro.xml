<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pantallaRegistroPerroVM" type="com.verdusapplication.app.modules.pantallaregistroperro.data.viewmodel.PantallaRegistroPerroVM"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearColumnvendedog"
        style="@style/groupStylegray_900_8c"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_3pxh"
        android:layout_marginBottom="@dimen/_35pxv"
        android:gravity="center_horizontal">

        <FrameLayout
            android:id="@+id/frameStackvendedog"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="9dp"
            android:gravity="start"
            app:layout_constraintBottom_toTopOf="@+id/linearColumnrectanglethirtytwo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                style="@style/groupStylebluegray_100"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="top|start"
                android:layout_marginBottom="@dimen/_10pxv"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRowvendedog"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtVendedog"
                        style="@style/txtInterextrabold28_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_weight="0.61"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtVendedog}"
                        tools:text="@string/lbl_vendedog" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnCerrarSesin"
                        style="@style/btnSolidRoundedOutline_1"
                        android:layout_width="@dimen/_92pxh"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginStart="@dimen/_99pxh"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cerrar_sesi_n"
                        android:textAllCaps="false"
                        tools:text="@string/lbl_cerrar_sesi_n" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <ImageView
                android:id="@+id/imageLOGO"
                imageRes="@{@drawable/img_logo_54x70}"
                android:layout_width="@dimen/_70pxh"
                android:layout_height="@dimen/_54pxh"
                android:layout_gravity="top|start"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_logo_54x70" />

            <FrameLayout
                android:id="@+id/frameStackarrowleft"
                style="@style/groupStylegray_400"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40pxv"
                android:layout_gravity="bottom|start"
                android:layout_marginTop="@dimen/_10pxv">

                <ImageView
                    android:id="@+id/imageArrowleft"
                    android:layout_width="@dimen/_10pxh"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_gravity="bottom|start"
                    android:layout_marginStart="@dimen/_33pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_33pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_arrowleft" />
            </FrameLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/linearColumnrectanglethirtytwo"
            style="@style/groupStylegray_900_8c"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:gravity="start"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/frameStackvendedog">

            <LinearLayout
                android:id="@+id/linearRowrectanglethirtytwo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <FrameLayout
                    android:id="@+id/frameStackrectanglethirtytwo"
                    android:layout_width="@dimen/_45pxh"
                    android:layout_height="@dimen/_39pxv">

                    <ImageView
                        android:id="@+id/imageRectangleThirtyTwo"
                        android:layout_width="@dimen/_45pxh"
                        android:layout_height="@dimen/_39pxh"
                        android:layout_gravity="start"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle32"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_rectangle32" />

                    <ImageView
                        android:id="@+id/imageSettings"
                        android:layout_width="@dimen/_28pxh"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_gravity="center"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_settings" />
                </FrameLayout>

                <TextView
                    android:id="@+id/txtAadirfoto"
                    style="@style/txtIntersemibold14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_weight="0.63"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtAadirfoto}"
                    tools:text="@string/lbl_a_adir_foto" />
            </LinearLayout>

            <TextView
                android:id="@+id/txtRaza"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtRaza}"
                tools:text="@string/lbl_raza" />

            <EditText
                android:id="@+id/etRectangleTwentyFour"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentyFourValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtEdad"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtEdad}"
                tools:text="@string/lbl_edad" />

            <EditText
                android:id="@+id/etRectangleTwentyFive"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentyFiveValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtColor"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtColor}"
                tools:text="@string/lbl_color" />

            <EditText
                android:id="@+id/etRectangleTwentySix"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentySixValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtRazapadre"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtRazapadre}"
                tools:text="@string/lbl_raza_padre" />

            <EditText
                android:id="@+id/etRectangleTwentySeven"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentySevenValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtColorpadre"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtColorpadre}"
                tools:text="@string/lbl_color_padre" />

            <EditText
                android:id="@+id/etRectangleTwentyEight"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentyEightValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtRazamadre"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtRazamadre}"
                tools:text="@string/lbl_raza_madre" />

            <EditText
                android:id="@+id/etRectangleTwentyNine"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleTwentyNineValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtColormadre"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtColormadre}"
                tools:text="@string/lbl_color_madre" />

            <EditText
                android:id="@+id/etRectangleThirty"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleThirtyValue}"
                tools:ignore="Autofill" />

            <TextView
                android:id="@+id/txtLocalizacintr"
                style="@style/txtIntersemibold14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{pantallaRegistroPerroVM.pantallaRegistroPerroModel.txtLocalizacintr}"
                tools:text="@string/lbl_localizaci_ntr" />

            <EditText
                android:id="@+id/etRectangleThirtyOne"
                style="@style/etSolid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:background="@drawable/rectangle_bg_bluegray_100"
                android:ellipsize="end"
                android:inputType="text"
                android:singleLine="true"
                android:text="@={pantallaRegistroPerroVM.pantallaRegistroPerroModel.etRectangleThirtyOneValue}"
                tools:ignore="Autofill" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnRegistrar"
                style="@style/btnSolidRoundedOutline_2"
                android:layout_width="@dimen/_92pxh"
                android:layout_height="@dimen/_40pxv"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_12pxv"
                android:paddingEnd="@dimen/_18pxh"
                android:paddingBottom="@dimen/_12pxv"
                android:singleLine="true"
                android:text="@string/lbl_registrar"
                android:textAllCaps="false"
                tools:text="@string/lbl_registrar" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
