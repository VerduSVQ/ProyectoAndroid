<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pantallaPrincipLMenuVM" type="com.verdusapplication.app.modules.pantallaprinciplmenu.data.viewmodel.PantallaPrincipLMenuVM"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearColumncompradog"
        style="@style/groupStylegray_900_8c"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_3pxh"
        android:gravity="center_horizontal">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="14dp"
            android:gravity="start"
            app:contentInsetStart="0dp"
            app:layout_constraintBottom_toTopOf="@+id/linearPantallaprinci"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <FrameLayout
                android:id="@+id/frameStackcompradog"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/linearRowcompradog"
                    style="@style/groupStylebluegray_100"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/txtCompradog"
                        style="@style/txtInterextrabold28_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_weight="0.64"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLMenuVM.pantallaPrincipLMenuModel.txtCompradog}"
                        tools:text="@string/lbl_compradog" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnCerrarSesin"
                        style="@style/btnSolidRoundedOutline_1"
                        android:layout_width="@dimen/_92pxh"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginStart="@dimen/_78pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cerrar_sesi_n"
                        android:textAllCaps="false"
                        tools:text="@string/lbl_cerrar_sesi_n" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imageLOGO"
                    imageRes="@{@drawable/img_logo_2}"
                    android:layout_width="@dimen/_70pxh"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_logo_2" />

                <LinearLayout
                    android:id="@+id/linearRowsearch"
                    style="@style/groupStylegray_400"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|start"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageSearch"
                        android:layout_width="@dimen/_25pxh"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_search" />

                    <TextView
                        android:id="@+id/txtGroupTwentyThree"
                        style="@style/txtSolid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_marginEnd="@dimen/_287pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_weight="0.8"
                        android:ellipsize="end"
                        android:paddingStart="@dimen/_6pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:paddingBottom="@dimen/_4pxv"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLMenuVM.pantallaPrincipLMenuModel.txtGroupTwentyThree}"
                        tools:text="@string/lbl_buscar" />
                </LinearLayout>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>

        <LinearLayout
            android:id="@+id/linearPantallaprinci"
            style="@style/groupStylegray_900_8c"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="33dp"
            android:layout_marginLeft="33dp"
            android:layout_marginEnd="33dp"
            android:layout_marginRight="33dp"
            android:layout_marginBottom="32dp"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/toolbarToolbar">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListrectangleten"
                itemSpace="@{@dimen/_15pxh}"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:itemCount="4"
                tools:listitem="@layout/row_listrectangleten" />
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
