<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pantallaPrincipLMenuVM" type="com.verdusapplication.app.modules.pantallaprinciplmenu.data.viewmodel.PantallaPrincipLMenuVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumncompradog"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_102pxv"
        android:layout_marginEnd="@dimen/_3pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900_8c">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <FrameLayout android:id="@+id/frameStackcompradog"
                android:layout_height="@dimen/_93pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowcompradog"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylebluegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCompradog"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLMenuVM.pantallaPrincipLMenuModel.txtCompradog}"
                        style="@style/txtInterextrabold28_1" tools:text="@string/lbl_compradog"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnCerrarSesin"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_78pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_92pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cerrar_sesi_n"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_cerrar_sesi_n"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageLOGO"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_width="@dimen/_70pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logo_2}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo_2"/>
                <LinearLayout android:id="@+id/linearRowsearch"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegray_400">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtGroupTwentyThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_287pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_4pxv"
                        android:paddingStart="@dimen/_6pxh"
                        android:paddingTop="@dimen/_1pxv"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLMenuVM.pantallaPrincipLMenuModel.txtGroupTwentyThree}"
                        style="@style/txtSolid" tools:text="@string/lbl_buscar"/>
                </LinearLayout>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearPantallaprinci"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_36pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_900_8c">
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start"
                android:id="@+id/recyclerListrectangleten"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_15pxh}" tools:itemCount="4" tools:listitem="@layout/row_listrectangleten"/>
        </LinearLayout>
    </LinearLayout>
</layout>
