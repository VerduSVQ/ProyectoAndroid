<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pantallaPrincipLCompraOneVM" type="com.verdusapplication.app.modules.pantallaprinciplcompraone.data.viewmodel.PantallaPrincipLCompraOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumncompradog"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_49pxv"
        android:layout_marginEnd="@dimen/_3pxh"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_900_8c">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <FrameLayout android:id="@+id/frameStackcompradog"
                android:layout_height="@dimen/_93pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearRowcompradog"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylebluegray_100">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCompradog"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_76pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtCompradog}"
                        style="@style/txtInterextrabold28_1" tools:text="@string/lbl_compradog"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnCerrarSesin"
                        android:layout_height="@dimen/_40pxv"
                        android:layout_marginBottom="@dimen/_8pxv"
                        android:layout_marginEnd="@dimen/_17pxh"
                        android:layout_marginStart="@dimen/_78pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_92pxh"
                        android:paddingBottom="@dimen/_12pxv"
                        android:paddingEnd="@dimen/_5pxh"
                        android:paddingStart="@dimen/_8pxh"
                        android:paddingTop="@dimen/_12pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_cerrar_sesi_n"
                        android:textAllCaps="false"
                        style="@style/btnSolidRoundedOutline_1" tools:text="@string/lbl_cerrar_sesi_n"/>
                </LinearLayout>
                <ImageView android:id="@+id/imageLOGO"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_54pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_width="@dimen/_70pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_logo_3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_logo_3"/>
                <FrameLayout android:id="@+id/frameStackarrowleft"
                    android:layout_gravity="bottom|start"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" style="@style/groupStylegray_400">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_gravity="bottom|start"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_33pxh"
                        android:layout_marginStart="@dimen/_33pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_10pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                </FrameLayout>
            </FrameLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearPantallaprinci"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_900_8c">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumnrectangleten"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylebluegray_100cornerRadius">
                <ImageView android:id="@+id/imageRectangleTen"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_264pxh"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="@dimen/_285pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle10}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle10"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnlanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_12pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:id="@+id/txtLanguage"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtLanguage}"
                        style="@style/txtInterextrabold48" tools:text="@string/lbl_pastor_belga"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowprice"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.55"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtPrice}"
                            style="@style/txtInterblack36" tools:text="@string/lbl_400"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtMesesCounter"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_7pxv"
                            android:layout_marginStart="@dimen/_85pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_weight="0.45"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtMesesCounter}"
                            style="@style/txtInterblack20_1" tools:text="@string/lbl_3_meses"/>
                    </LinearLayout>
                    <TextView android:id="@+id/txtDescription"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_13pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtDescription}"
                        style="@style/txtInterextralight20" tools:text="@string/msg_caracter_sticas"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtVendidopor"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_11pxh"
                        android:layout_marginTop="@dimen/_56pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtVendidopor}"
                        style="@style/txtInterregular20" tools:text="@string/lbl_vendido_por"/>
                    <LinearLayout android:id="@+id/linearRowllamar"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_11pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylegray_500">
                        <LinearLayout
                            android:id="@+id/linearRowrectangletwentyone"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_weight="0.59"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageRectangleTwentyOne"
                                android:layout_height="@dimen/_35pxh"
                                android:layout_width="@dimen/_35pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle21"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle21"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtJosLuis"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_5pxv"
                                android:layout_marginStart="@dimen/_9pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_weight="0.72"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{pantallaPrincipLCompraOneVM.pantallaPrincipLCompraOneModel.txtJosLuis}"
                                style="@style/txtInterregular20_1" tools:text="@string/lbl_jos_luis"/>
                        </LinearLayout>
                        <androidx.appcompat.widget.AppCompatButton
                            android:ellipsize="end"
                            android:gravity="center"
                            android:id="@+id/btnLlamar"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_104pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_width="@dimen/_92pxh"
                            android:paddingBottom="@dimen/_12pxv"
                            android:paddingEnd="@dimen/_26pxh"
                            android:paddingStart="@dimen/_26pxh"
                            android:paddingTop="@dimen/_12pxv"
                            android:singleLine="true"
                            android:text="@string/lbl_llamar"
                            android:textAllCaps="false"
                            style="@style/btnSolidRoundedOutline_2" tools:text="@string/lbl_llamar"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
